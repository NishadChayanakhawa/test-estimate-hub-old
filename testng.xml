<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Test Estimate Hub Tests" parallel="tests" thread-count="5">
	<listeners>
		<listener
			class-name="io.nishadc.automationtestingframework.testngcustomization.ReportGenerator"></listener>
	</listeners>
	<test name="Configuration Data Tests" parallel="methods" thread-count="5">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.unittests.modelTests.ApplicationConfigurationServiceTests" />
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.unittests.modelTests.TestTypeServiceTests" />
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.unittests.modelTests.ChangeTypeServiceTests" />
		</classes>
	</test>
	<test name="Release Data Tests" parallel="methods" thread-count="5">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.unittests.modelTests.ReleaseServiceTests" />
		</classes>
	</test>
	<test name="Change Data Tests" parallel="methods" thread-count="5">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.unittests.modelTests.ChangeServiceTests" />
		</classes>
	</test>
	<test name="Requirement Data Tests" parallel="methods" thread-count="5">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.unittests.modelTests.RequirementDataTests" />
		</classes>
	</test>
</suite>
